{
  "name": "saas-factory",
  "version": "1.0.0",
  "description": "Modern, Clean Architecture for AI-Powered SaaS Applications",
  "private": true,
  "workspaces": [
    "ui/nextjs"
  ],
  "scripts": {
    "dev": "cd ui/nextjs && npm run dev",
    "build": "cd ui/nextjs && npm run build",
    "start": "cd ui/nextjs && npm run start",
    "lint": "cd ui/nextjs && npm run lint",
    "test": "cd ui/nextjs && npm run test",
    "type-check": "cd ui/nextjs && npm run type-check",
    "clean": "rm -rf ui/nextjs/.next ui/nextjs/node_modules/.cache",
    "install-all": "npm install && cd ui/nextjs && npm install",
    "update-deps": "cd ui/nextjs && npm update",
    "supabase:start": "cd ui/nextjs && npx supabase start",
    "supabase:stop": "cd ui/nextjs && npx supabase stop",
    "supabase:status": "cd ui/nextjs && npx supabase status",
    "db:migrate": "cd ui/nextjs && npx supabase db reset",
    "db:generate-types": "cd ui/nextjs && npx supabase gen types typescript --local > supabase/types/database.ts",
    "edge:deploy": "cd ui/nextjs && npx supabase functions deploy ai-agent",
    "edge:logs": "cd ui/nextjs && npx supabase functions logs ai-agent",
    "python:install": "pip install -r requirements.txt",
    "python:install-dev": "pip install -r requirements.txt[dev]",
    "python:setup": "python setup.py develop",
    "python:test": "pytest",
    "python:lint": "black . && isort . && flake8 .",
    "python:format": "black . && isort ."
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "saas",
    "ai",
    "react",
    "nextjs",
    "supabase",
    "typescript",
    "tailwindcss"
  ],
  "author": "SaaS Factory Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/saas-factory.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/saas-factory/issues"
  },
  "homepage": "https://github.com/your-org/saas-factory#readme"
}
